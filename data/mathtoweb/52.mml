<?xml version="1.0" encoding="UTF-8"?>
<math xmlns="http://www.w3.org/1998/Math/MathML">
   <mrow xmlns="">
      <mstyle displaystyle="true">
         <munder>
            <mrow>
               <munder accentunder="true">
                  <mrow>
                     <msub>
                        <mi>u</mi>
                        <mn>1</mn>
                     </msub>
                     <mrow>
                        <mo form="prefix">(</mo>
                        <mi mathvariant="bold">x</mi>
                        <mo>,</mo>
                        <msub>
                           <mi>z</mi>
                           <mn>1</mn>
                        </msub>
                        <mo form="postfix">)</mo>
                     </mrow>
                     <mo>=</mo>
                     <msub>
                        <mi>v</mi>
                        <mn>1</mn>
                     </msub>
                     <mo>+</mo>
                     <msub>
                        <mrow>
                           <mover accent="true">
                              <mi>u</mi>
                              <mo>˙</mo>
                           </mover>
                        </mrow>
                        <mi>x</mi>
                     </msub>
                  </mrow>
                  <mo stretchy="true">︸</mo>
               </munder>
            </mrow>
            <mrow>
               <mtext>By definition of </mtext>
               <msub>
                  <mi>v</mi>
                  <mn>1</mn>
               </msub>
            </mrow>
         </munder>
      </mstyle>
      <mo>=</mo>
      <mstyle displaystyle="true">
         <mover>
            <mrow>
               <mover accent="true">
                  <mrow>
                     <mo>-</mo>
                     <mfrac linethickness="1">
                        <mrow>
                           <mo>∂</mo>
                           <msub>
                              <mi>V</mi>
                              <mi>x</mi>
                           </msub>
                        </mrow>
                        <mrow>
                           <mo>∂</mo>
                           <mi mathvariant="bold">x</mi>
                        </mrow>
                     </mfrac>
                     <msub>
                        <mi>g</mi>
                        <mi>x</mi>
                     </msub>
                     <mrow>
                        <mo form="prefix">(</mo>
                        <mi mathvariant="bold">x</mi>
                        <mo form="postfix">)</mo>
                     </mrow>
                     <mo>-</mo>
                     <msub>
                        <mi>k</mi>
                        <mn>1</mn>
                     </msub>
                     <mrow>
                        <mo form="prefix">(</mo>
                        <mstyle displaystyle="true">
                           <munder>
                              <mrow>
                                 <munder accentunder="true">
                                    <mrow>
                                       <msub>
                                          <mi>z</mi>
                                          <mn>1</mn>
                                       </msub>
                                       <mo>-</mo>
                                       <msub>
                                          <mi>u</mi>
                                          <mi>x</mi>
                                       </msub>
                                       <mo form="prefix">(</mo>
                                       <mi mathvariant="bold">x</mi>
                                       <mo form="postfix">)</mo>
                                    </mrow>
                                    <mo stretchy="true">︸</mo>
                                 </munder>
                              </mrow>
                              <mrow>
                                 <msub>
                                    <mi>e</mi>
                                    <mn>1</mn>
                                 </msub>
                              </mrow>
                           </munder>
                        </mstyle>
                        <mo form="postfix">)</mo>
                     </mrow>
                  </mrow>
                  <mo stretchy="true">︷</mo>
               </mover>
            </mrow>
            <mrow>
               <msub>
                  <mi>v</mi>
                  <mn>1</mn>
               </msub>
            </mrow>
         </mover>
      </mstyle>
      <mspace width="0.167em"/>
      <mo>+</mo>
      <mspace width="0.167em"/>
      <mstyle displaystyle="true">
         <mover>
            <mrow>
               <mover accent="true">
                  <mrow>
                     <mfrac linethickness="1">
                        <mrow>
                           <mo>∂</mo>
                           <msub>
                              <mi>u</mi>
                              <mi>x</mi>
                           </msub>
                        </mrow>
                        <mrow>
                           <mo>∂</mo>
                           <mi mathvariant="bold">x</mi>
                        </mrow>
                     </mfrac>
                     <mrow>
                        <mo form="prefix">(</mo>
                        <mstyle displaystyle="true">
                           <munder>
                              <mrow>
                                 <munder accentunder="true">
                                    <mrow>
                                       <msub>
                                          <mi>f</mi>
                                          <mi>x</mi>
                                       </msub>
                                       <mo form="prefix">(</mo>
                                       <mi mathvariant="bold">x</mi>
                                       <mo form="postfix">)</mo>
                                       <mo>+</mo>
                                       <msub>
                                          <mi>g</mi>
                                          <mi>x</mi>
                                       </msub>
                                       <mo form="prefix">(</mo>
                                       <mi mathvariant="bold">x</mi>
                                       <mo form="postfix">)</mo>
                                       <msub>
                                          <mi>z</mi>
                                          <mn>1</mn>
                                       </msub>
                                    </mrow>
                                    <mo stretchy="true">︸</mo>
                                 </munder>
                              </mrow>
                              <mrow>
                                 <mover accent="true">
                                    <mrow>
                                       <mi mathvariant="bold">x</mi>
                                    </mrow>
                                    <mo>˙</mo>
                                 </mover>
                                 <mtext> (i.e., </mtext>
                                 <mfrac linethickness="1">
                                    <mi>d</mi>
                                    <mi mathvariant="bold">x</mi>
                                    <mrow>
                                       <mi>d</mi>
                                       <mi>t</mi>
                                    </mrow>
                                 </mfrac>
                                 <mtext>)</mtext>
                              </mrow>
                           </munder>
                        </mstyle>
                        <mo form="postfix">)</mo>
                     </mrow>
                  </mrow>
                  <mo stretchy="true">︷</mo>
               </mover>
            </mrow>
            <mrow>
               <msub>
                  <mrow>
                     <mover accent="true">
                        <mi>u</mi>
                        <mo>˙</mo>
                     </mover>
                  </mrow>
                  <mi>x</mi>
               </msub>
               <mtext> (i.e., </mtext>
               <mfrac linethickness="1">
                  <mi>d</mi>
                  <msub>
                     <mi>u</mi>
                     <mi>x</mi>
                  </msub>
                  <mrow>
                     <mi>d</mi>
                     <mi>t</mi>
                  </mrow>
               </mfrac>
               <mtext>)</mtext>
            </mrow>
         </mover>
      </mstyle>
   </mrow>
</math>
